{
  "name": "Fasterize Status",
  "version": "4.1.3",
  "manifest_version": 3,
  "description": "See detailed information on a fasterized page",
  "icons": {
    "16": "icons/store/icon16.png",
    "48": "icons/store/icon48.png",
    "128": "icons/store/icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Fasterize",
    "default_icon": "icons/store/icon.png"
  },
  "background": {
    "service_worker": "main.js"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "cookies",
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "vendor/browser-polyfill.js",
        "contentscript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "show_fstrz_traces.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "incognito": "spanning"
}